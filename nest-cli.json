        "tsConfigPath": "apps/read-episode/tsconfig.app.json"
      }
    },
    "pattern-dashboard": {
      "type": "application",
      "root": "apps/pattern-dashboard",
      "entryFile": "main",
      "sourceRoot": "apps/pattern-dashboard/src",
      "compilerOptions": {
        "tsConfigPath": "apps/pattern-dashboard/tsconfig.app.json"
      }
    },
    "data-access": {
      "type": "library",
      "root": "libs/data-access",
      "entryFile": "index",
      "sourceRoot": "libs/data-access/src",
      "compilerOptions": {
        "tsConfigPath": "libs/data-access/tsconfig.lib.json"
      }

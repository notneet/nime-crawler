{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nime-crawler/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "apps/nime-crawler/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nime-crawler",
  "projects": {
    "nime-crawler": {
      "type": "application",
      "root": "apps/nime-crawler",
      "entryFile": "main",
      "sourceRoot": "apps/nime-crawler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nime-crawler/tsconfig.app.json"
      }
    },
    "cron-interval": {
      "type": "application",
      "root": "apps/cron-interval",
      "entryFile": "main",
      "sourceRoot": "apps/cron-interval/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cron-interval/tsconfig.app.json"
      }
    },
    "helpers": {
      "type": "library",
      "root": "libs/helpers",
      "entryFile": "index",
      "sourceRoot": "libs/helpers/src",
      "compilerOptions": {
        "tsConfigPath": "libs/helpers/tsconfig.lib.json"
      }
    },
    "commons": {
      "type": "library",
      "root": "libs/commons",
      "entryFile": "index",
      "sourceRoot": "libs/commons/src",
      "compilerOptions": {
        "tsConfigPath": "libs/commons/tsconfig.lib.json"
      }
    },
    "entities": {
      "type": "library",
      "root": "libs/entities",
      "entryFile": "index",
      "sourceRoot": "libs/entities/src",
      "compilerOptions": {

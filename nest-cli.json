{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nime-crawler/src",
  "projects": {
    "commons": {
      "type": "library",
      "root": "libs/commons",
      "entryFile": "index",
      "sourceRoot": "libs/commons/src",
      "compilerOptions": {
        "tsConfigPath": "libs/commons/tsconfig.lib.json"
      }
    },
    "nime-crawler": {
      "type": "application",
      "root": "apps/nime-crawler",
      "entryFile": "main",
      "sourceRoot": "apps/nime-crawler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nime-crawler/tsconfig.app.json"
      }
    },
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "cron-interval": {
      "type": "application",
      "root": "apps/cron-interval",
      "entryFile": "main",
      "sourceRoot": "apps/cron-interval/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cron-interval/tsconfig.app.json"
      }
    },
    "read-anime-source": {
      "type": "application",
      "root": "apps/read-anime-source",
      "entryFile": "main",
      "sourceRoot": "apps/read-anime-source/src",
      "compilerOptions": {
        "tsConfigPath": "apps/read-anime-source/tsconfig.app.json"
      }
    },
    "routing-queue": {
      "type": "application",
      "root": "apps/routing-queue",
      "entryFile": "main",
      "sourceRoot": "apps/routing-queue/src",
      "compilerOptions": {
        "tsConfigPath": "apps/routing-queue/tsconfig.app.json"
      }
    },
    "scraper-service": {
      "type": "application",
      "root": "apps/scraper-service",
      "entryFile": "main",
      "sourceRoot": "apps/scraper-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/scraper-service/tsconfig.app.json"
      }
    },
    "scraper-stream": {
      "type": "application",
      "root": "apps/scraper-stream",
      "entryFile": "main",
      "sourceRoot": "apps/scraper-stream/src",
      "compilerOptions": {
        "tsConfigPath": "apps/scraper-stream/tsconfig.app.json"
      }
    },
    "media-updater": {
      "type": "application",
      "root": "apps/media-updater",
      "entryFile": "main",
      "sourceRoot": "apps/media-updater/src",
      "compilerOptions": {
        "tsConfigPath": "apps/media-updater/tsconfig.app.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "apps/nime-crawler/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nime-crawler"
}
      }
    },
    "read-index": {
      "type": "application",
      "root": "apps/read-index",
      "entryFile": "main",
      "sourceRoot": "apps/read-index/src",
      "compilerOptions": {
        "tsConfigPath": "apps/read-index/tsconfig.app.json"
      }
    },
    "read-detail": {
      "type": "application",
      "root": "apps/read-detail",
      "entryFile": "main",
      "sourceRoot": "apps/read-detail/src",
      "compilerOptions": {
        "tsConfigPath": "apps/read-detail/tsconfig.app.json"
      }
    },
    "read-link": {
      "type": "application",
      "root": "apps/read-link",

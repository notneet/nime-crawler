{
  "name": "nime-crawler",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "echo 'Warn: Build using ./scripts/build'",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/nime-crawler/main",
    "start:dev:api": "nest start --watch api",
    "start:dev:cron-interval": "nest start --watch cron-interval",
    "start:dev:media-updater": "nest start --watch media-updater",
    "start:dev:routing-queue": "nest start --watch routing-queue",
    "start:dev:scraper-service:post": "SCRAPE_TYPE=post nest start --watch scraper-service",
    "start:dev:scraper-service:detail": "SCRAPE_TYPE=detail nest start --watch scraper-service",
    "start:dev:scraper-service:episode": "SCRAPE_TYPE=episode nest start --watch scraper-service",
    "start:dev:scraper-stream": "nest start --watch scraper-stream",
    "build:api": "nest build api",
    "build:cron-interval": "nest build cron-interval",
    "build:media-updater": "nest build media-updater",
    "build:routing-queue": "nest build routing-queue",
    "build:scraper-service": "nest build scraper-service",
    "build:scraper-stream": "nest build scraper-stream",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/nime-crawler/test/jest-e2e.json",
    "postinstall": "ts-patch install"
  },
  "dependencies": {
    "@nestia/core": "^3.1.4",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/microservices": "^10.3.8",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/typeorm": "^10.0.2",
    "@sentry/node": "^7.114.0",
    "@sentry/tracing": "^7.114.0",
    "@travelerdev/nestjs-sentry": "^4.3.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0-rc.12",
    "chrono-node": "^2.7.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "https-proxy-agent": "^7.0.4",
    "libxmljs2": "^0.33.0",
    "luxon": "^3.4.4",
    "minio": "^8.0.0",
    "mysql2": "^3.9.7",
    "parse5": "^7.1.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "random-useragent": "^0.5.0",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestia/e2e": "^0.4.3",
    "@nestia/sdk": "^3.1.4",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/amqplib": "^0.10.5",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "^20.12.11",
    "@types/passport-jwt": "^4.0.1",
    "@types/random-useragent": "^0.3.3",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "29.7.0",
    "nestia": "^5.3.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@libs/commons(|/.*)$": "<rootDir>/libs/commons/src/$1"
    }
  }
}

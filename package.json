  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/nime-crawler/main",
    "start:dev:api": "nest start --watch api",
    "start:dev:cron-interval": "nest start --watch cron-interval",
    "start:dev:read-anime-source": "nest start --watch read-anime-source",
    "start:dev:routing-queue": "nest start --watch routing-queue",
    "start:dev:scraper-service": "nest start --watch scraper-service",
    "start:dev:scraper-stream": "nest start --watch scraper-stream",
    "build:api": "nest build api",
    "build:cron-interval": "nest build cron-interval",
    "build:read-anime-source": "nest build read-anime-source",
    "build:routing-queue": "nest build routing-queue",
    "build:scraper-service": "nest build scraper-service",
    "build:scraper-stream": "nest build scraper-stream",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/nime-crawler/test/jest-e2e.json",
    "postinstall": "ts-patch install"
  },
  "dependencies": {
    "@nestia/core": "^2.1.5",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.2.1",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.1",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/microservices": "^10.2.1",
    "@nestjs/platform-express": "^10.2.1",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/typeorm": "^10.0.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "https-proxy-agent": "^7.0.1",
    "libxmljs2": "^0.32.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mysql2": "^3.6.0",
    "parse5": "^7.1.2",
    "random-useragent": "^0.5.0",
    "redis": "^4.6.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestia/e2e": "^0.3.6",
    "@nestia/sdk": "^2.1.5",
    "@nestjs/cli": "^10.1.16",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.1",
    "@types/amqplib": "^0.10.1",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.4",
    "@types/node": "^20.5.6",
    "@types/random-useragent": "^0.3.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "29.6.4",
    "nestia": "^4.5.0",
    "prettier": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/libs/",
      "<rootDir>/apps/"
    ],
    "moduleNameMapper": {
      "^@libs/commons(|/.*)$": "<rootDir>/libs/commons/src/$1"
    }
  }
}
